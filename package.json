    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "graphql": "^16.3.0",
    "prettier": "^2.5.1",
    "rollup": "^2.66.1",
    "rollup-plugin-node-externals": "^3.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "urql": "^2.0.6",
    "xo": "^0.47.0"
