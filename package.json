{
  "name": "urql-generic-requester",
  "version": "0.0.0-local",
  "description": "GraphQL Codegen Generic SDK Requeter creator for URQL Client",
  "license": "MIT",
  "author": "Reaper <ahoy@barelyhuman.dev>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "index.ts",
  "keywords": [
    "graphql",
